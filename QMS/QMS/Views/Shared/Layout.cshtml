                        @foreach (var sm in subModules)
                        {
                            var itemName = "";
                            if (permissions.Any(p => p.SubModule_Id == sm.Id && p.Role_Id == userRole && p.AccessLevel > 0))
                            {
                                if (sm.Name.EndsWith("s"))
                                {
                                    itemName = sm.Name.Remove(sm.Name.Length - 1, 1);
                                }
                                else
                                {
                                    itemName = sm.Name;
                                }

                                switch (itemName)
                                {
                                    case "Employee":
                                        DefinitionMenu.Add("Employee");
                                        break;

                                    case "Department":
                                        DefinitionMenu.Add("Department");
                                        break;

                                    case "Job Title":
                                        DefinitionMenu.Add("Job Title");
                                        break;

                                    case "ComplaintMainType":
                                        DefinitionMenu.Add("Complaint Main Type");
                                        break;

                                    case "ComplaintSubType":
                                        DefinitionMenu.Add("Complaint Sub Type");
                                        break;

                                    case "Complaint":
                                        ComplaintMenu.Add("Complaints");
                                        break;

                                    case "Report":
                                        ReportMenu.Add("Reports");
                                        break;


                                    case "Role":
                                        SecurityMenu.Add("Role");
                                        break;

                                    case "SystemUser":
                                        SecurityMenu.Add("System User");
                                        break;


                                    case "UserPermission":
                                        SecurityMenu.Add("User Permission");
                                        break;

                                }

                            }
                        }

                        @if (DefinitionMenu.Count > 0)
                        {
                            <li class=" w-full h-full py-3  border-b border-light-border hover:bg-white ">
                                <a href="#DefinitionSection" data-toggle="collapse" id="ultogglerOther"
