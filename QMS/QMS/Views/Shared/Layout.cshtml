    function modalPassOpenFunction() {
        $('#suc').addClass('hide');
        $('#fail').addClass('hide');
        $('#oldPassword').val("");
        $('#password').val("");
        $('#confirmPassword').val("");
        $('#myModalPassword').modal('show');
    }

    function VerifyPassword() {
        
        $('#fail').addClass('hide');
        var pass = document.getElementById("password").value; 
       
        var newPass = document.getElementById("confirmPassword").value; 
        
        if (pass != newPass) {
            alert('New Password does not match, Please enter again');
            return
        }
        else {

            var OldPass = $('#oldPassword').val();
            var NewPass = $('#password').val();
            

            var Url = '@Url.Content("~/User/ChangePasswordAjax")';
            //alert(Url);
            $.ajax({
                url: Url,
                type: 'POST',
                data: { oldPass: OldPass, newPass: NewPass },
                success: function (response) {
                    var data = JSON.parse(response);
                    
                    if (data == true) {
                        $('#oldPassword').val("");
                        $('#password').val("");
                        $('#confirmPassword').val("");
                        $('#suc').removeClass('hide');
                        $('#fail').addClass('hide');
                        //$('#oldPassword').focus();
                        
                    }
                    else {
                        //$('#oldPassword').focus();
                        $('#suc').addClass('hide');
                        $('#fail').removeClass('hide');
                        
                    }
                    
                  
                },
                error: function () {
                    
                }
            });


            //$('#btnChangePass').trigger('click');
        }
    }

